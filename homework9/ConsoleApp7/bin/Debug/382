			document.domain = 'qq.com';

			var _su,suin,checklogin_r = /\D/g;
			suin = (_su=QZFL.cookie.get('uin').replace(checklogin_r, '')-0) && QZFL.cookie.get('skey') && _su > 10000 && _su || 0;
			if(!suin){
				QZFL.imports(location.protocol + '//qzs.qq.com/ac/qzone/widget/login/login.js',function(){
					QZFL.widget.loginBox.show({
						callback: function() {
							applyBeta();
						}
					});
				});
				return;
			}

			QZFL.dialog.create("新版体验报名", '<div class="fn-txt"><div class="txt"><h2>尊敬的用户：</h2><p>我们诚邀您加入QQ空间体验改进计划。</p></div><div class="txt"><h2>报名成功后将享受以下特权：</h2><p>1、新版本内测资格</p><p>2、优先给您发下升级提醒</p><p>3、您的反馈我们将优先关注</p></div><p class="highlight">提交反馈体验还有机会获得意外小奖励哦！</p></div>',{
				buttonConfig : [{
						type : QZFL.dialog.BUTTON_TYPE.Confirm,
						text : '我要报名',
						tips : '新版体验报名',
						clickFn : function(){
							var fs = new QZFL.FormSender(location.protocol + '//3g.qzone.qq.com/client/mobile_register_grey','POST',{uin:suin,source:1},'UTF-8');
							fs.onSuccess = function(o){
								if(o.code==0){
									QZFL.widget.msgbox.show('报名成功', 4, 3000);
								}else{
									QZFL.widget.msgbox.show(o.message, 1, 3000);
								}
							};
							fs.onError = function(o){
								QZFL.widget.msgbox.show('系统繁忙，请稍后再试。', 5, 3000);
							};
							fs.send();
						}
					},{
						type : QZFL.dialog.BUTTON_TYPE.Cancel,
						text : '暂不参加',
						tips : '暂不参加'
					}
				],
				showMask : true,
				width : 380,
				height : 270
			});
			TCISD&&TCISD.hotClick('mobileProtal.index_betaapply','sns.qzone.qq.com');
		}
	}
	if(location.hash=='#applybeta'){
		applyBeta();
	} 
</script>
