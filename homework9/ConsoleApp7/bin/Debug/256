<script type="text/javascript">
	
	function welcome(id){
		var now = new Date(),hour = now.getHours();
		var dayNames = new Array("星期日","星期一","星期二","星期三","星期四","星期五","星期六"); 
		var welcome =now.getFullYear() + "年"+(now.getMonth() + 1) +"月"+now.getDate()+ "日"+ " " + dayNames[now.getDay()] +" ";
		if(hour < 6){document.write("凌晨好！")} 
		else if (hour < 9) {welcome+="早上好！"} 
		else if (hour < 12){welcome+="上午好！"} 
		else if (hour < 14){welcome+="中午好！"} 
		else if (hour < 17){welcome+="下午好！"} 
		else if (hour < 19){welcome+="傍晚好！"} 
		else if (hour < 22){welcome+="晚上好！"} 
		else {welcome+="深夜好！"};
		$(id).html(welcome);
	}
</script>
</head>
