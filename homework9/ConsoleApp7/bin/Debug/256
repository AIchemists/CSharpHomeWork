       					$("#float-pic2").show();
       				}
        		}
        	});
        	
        	$.ajax({
        		url:basepath + "/web/getHomePicNews?type=3",
        		success:function(data){
        			var sdata = data.data;
        			var html = "";
        			if(sdata){
        				for(var i = 0 ; i < sdata.length ; i++)
        				if(i%2 == 0 & i < 6){
        					html+='<li class="page-more no-margin-left" data-target="'+sdata[i].picurl+'"><a><img src="'+sdata[i].picsrc+'"/>'+sdata[i].title+'</a></li>'
        				}
        				else if(i%2 != 0 & i < 6){
							html+='<li class="page-more " data-target="'+sdata[i].picurl+'"><a><img src="'+sdata[i].picsrc+'"/>'+sdata[i].title+'</a></li>'
        				}
						
						$("#zfgkxf").html(html);
					}
        		}
        	});
        	
