        				}
		      	        html += '<li><a target="_blank" href="'+ this.picurl +'" title="'+this.title +'"><img src="'+ url +'"></a></li>';
        			})
        			html += "</ul>";
        			$("#index-pic-news").html(html);
        			 $('#index-pic-news').slideBox({
        			        delay : 8,//滚动延迟时间，单位：秒
