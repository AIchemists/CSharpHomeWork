<script>
	$(function(){
		
		window.onload = function()
		{
			var $tab_li = $('#tab li');
			var $tabul = $('#content .switchover');
			var $pagemore1 = $("#pagemore1 span");
			
			var $tab_li_entrance = $('#tab2 li');
			var $tabul_entrance = $('#content2 .switchover2');	
			var $pagemore2 = $("#pagemore2 span");
			
			
			var $tab_li_tab3 = $('#tab3 li');
			var $tabul_tab3 = $('#content3 .switchover3');	
			var $pagemore3 = $("#pagemore3 span");
			
			var $tab_li_tab10 = $('#tab10 li');
			var $tabul_tab10 = $('#content10 .switchover10');	
			var $pagemore10 = $("#pagemore10 span");
			
			
			$tab_li.mouseover(function(){
				var $this = $(this);
				var $t = $this.index();
				$tab_li.removeClass('act_active');
				$this.addClass('act_active');
				$tabul.css('display','none');
				$tabul.eq($t).css('display','block');
				
				$pagemore1.css('display','none');
				$pagemore1.eq($t).css('display','block');
			})
			$tab_li_entrance.mouseover(function(){
				var $this_entrance = $(this);
				var $t_entrance = $this_entrance.index();
				$tab_li_entrance.removeClass('act_active');
				$this_entrance.addClass('act_active');
				$tabul_entrance.css('display','none');
				$tabul_entrance.eq($t_entrance).css('display','block');
				
				$pagemore2.css('display','none');
				$pagemore2.eq($t_entrance).css('display','block');
				
			})
			
			$tab_li_tab3.mouseover(function(){
				var $this_entrance = $(this);
				
				var $t_entrance = $this_entrance.index();
				$tab_li_tab3.removeClass('act_active');
				$this_entrance.addClass('act_active');
				$tabul_tab3.css('display','none');
				$tabul_tab3.eq($t_entrance).css('display','block');
				
				
				$pagemore3.css('display','none');
				$pagemore3.eq($t_entrance).css('display','block');
			})
			
			$tab_li_tab10.mouseover(function(){
				var $this_entrance = $(this);
				
				var $t_entrance = $this_entrance.index();
				$tab_li_tab10.removeClass('act_active');
				$this_entrance.addClass('act_active');
				$tabul_tab10.css('display','none');
				$tabul_tab10.eq($t_entrance).css('display','block');
				
				
				$pagemore10.css('display','none');
				$pagemore10.eq($t_entrance).css('display','block');
			})
		}
	});  
	
	//绑定首页更多点击跳转事件
	//在需要调整的dom对象上配置 data-type="more"  data-mid=实际要跳转模块的id或者code就可以实现跳转
	$(".index-page-content").on("click",".page-more",function(){
		
		window.location.href = basepath +$(this).data("target");
		/* $.get(basepath+"/index/getToParentsMenus",{id:$(this).data("mid")},function(data){
			window.location.href = basepath +"/index?id="+data.id+"&code="+data.code+"&mid="+data.mid;
		}); */
	});
	


	
</script>

