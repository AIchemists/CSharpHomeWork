  			$("[data-res='index.jsp']").addClass("pitch_up");
  		}else if(requestUrl.indexOf("NewsFeatures.jsp") != -1){
  			$("[data-res='NewsFeatures.jsp']").addClass("pitch_up");
  		}else if(requestUrl.indexOf("Infomation.jsp") != -1){
  			$("[data-res='Infomation.jsp']").addClass("pitch_up");
  		}else if(requestUrl.indexOf("LawRule.jsp") != -1){
  			$("[data-res='LawRule.jsp']").addClass("pitch_up");
  		}else if(requestUrl.indexOf("InternetLaw.jsp") != -1){
  			$("[data-res='InternetLaw.jsp']").addClass("pitch_up");
  		}else if(requestUrl.indexOf("FeatureClass.jsp") != -1){
  			$("[data-res='FeatureClass.jsp']").addClass("pitch_up");
  		}else if(requestUrl.indexOf("Enterprises.jsp") != -1){
  			$("[data-res='Enterprises.jsp']").addClass("pitch_up");
  		}
  	})
  		
  </script>
