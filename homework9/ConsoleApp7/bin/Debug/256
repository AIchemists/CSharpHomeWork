<script type="text/javascript">
 layer.config({
	scrollbar: false,
	moveOut: true
	}); 
	 function initCms(code,target,maxnum,row,flag){
		 var html = "<ul>";
    	 var now = 0;
    	 if(!row)row =1;
    	 if(!maxnum)maxnum = 10;
    	 var asyncflag = true;
    	 
    	 var nowdate = new Date().Format('yyyy-MM-dd hh:mm:ss');
    	 if(flag=="false")asyncflag = false;
    	 $.ajax({
    		 	type:"post",
    			url:basepath+"/web/index/getWebIndexData.api",
    			data:{catogoryId:code,publicTime:nowdate,isNet:1},
    			async:asyncflag,
    			success:function(data){
    				var cmsDatas = data.data;
    				$.each(cmsDatas,function(i){
    		     		 /* if(this.catogoryId == code){  */
    		     			html+=" <div title='"+this.title+"'><li><a target='_blank' href="+gdcmsnetpath+this.address+">"+this.title+" </a><span style='font-size:13px;'>"+
    		     			    new Date(this.publicTime).Format("MM-dd")+"</span></li><div>"
    		     			now++;
    		     			if(now == maxnum && row == 1){
    		     				return false;
    		     			}else if(now == maxnum && row == 2){
    		     				html+= "</ul><ul>";
    		     			}else if(now == 2 * maxnum){
    		     				return false;
    		     			}
    		     		/* } */
    		     	});
    		     	 html += "</ul>";
    		     	$(target).html(html);
    		    }	
    			
    		});
	 }
	/* <p style="font-size:16px;">sadfasdfsadfdsaf</p>
		<marquee direction="up" height="210px;" behavior="scroll" scrollamount="2" scrolldelay="0" loop="-1"  hspace="10" vspace="10">
		 <p>指整个Marquee对齐方式;</p>
		 <p> (2)behavior:设置滚动的方式: scroll:表示由一端滚动到另一端,会重复,缺陷是不能无缝滚动。</p>
		  <p> slide:表示由一段滚动到另一端,不会重复...</p>
		</marquee>  */
 	function upDevolope(){
		$.ajax({
			type:"post",
			url:basepath+"/web/index/getWebIndexData.api?catogoryId=297",/* 256 */
			async:true,
			success:function(data){
				var sdata = data.data;
				var html = "<div class='huangcy'>";
				if(data){
					for(var i = 0 ; i < 4 ; i++){
						var title = "";
						if(sdata[i]){
							title = sdata[i].title.length > 8 ? sdata[i].title.substring(0,8)+"..." : sdata[i].title;
							if(i == "0" || i == "2"){
								html+="<div style='display:inline;' title='"+sdata[i].title+"'><a target='_blank' class='a_right_line' href='"+gdcmsnetpath+sdata[i].address+"'>"+title+"</a></div>";
							}
							if(i == "1"){
								html+="<div style='display:inline;' title='"+sdata[i].title+"'><a target='_blank' href='"+gdcmsnetpath+sdata[i].address+"'>"+title+"</a></div><div class='line'></div>";
							}
							if(i == "3"){
								html+="<div style='display:inline;' title='"+sdata[i].title+"'><a target='_blank' href='"+gdcmsnetpath+sdata[i].address+"'>"+title+"</a></div>";
							}
						}
					}
				}
				html+="</div>";
				$("#scroll-content-title").html(html); 
				
			}
		}); 
	} 
	 function initDevolope(code,target,maxnum,row,flag){
		 var html = "<ul>";
    	 var now = 0;
    	 if(!row)row =1;
    	 if(!maxnum)maxnum = 10;
    	 var asyncflag = true;
    	 var nowdate = new Date().Format('yyyy-MM-dd')+" 00:00:00";
    	 if(flag=="false")asyncflag = false;
    	 $.ajax({
    		 	type:"post",
    			url:basepath+"/web/index/getDetailContent.api",
    			data:{catogoryId:code,publicTime:nowdate},
    			async:asyncflag,
    			success:function(data){
    				var cmsDatas = data.data;
    				$.each(cmsDatas,function(i){
    		     		 if(this.catogoryId == code){ 
/*     		     			 var htmltitle = '<p style="font-size:17px;color:#066ab5;padding: 28px;border-bottom:1px solid #ddd;">'+this.title+'</p>'; 
    		     			$("#scroll-content-title").html(htmltitle); */
    		     			/* html += '<p style="font-size:16px;background: #f5f5f5;padding: 4px;">'+this.title+'</p>'; */
    		     			 html+= '<div class="list_lh"><ul style="margin-top :0px;"><li>'
    		     			/* html += '<marquee direction="up" height="210px;" onmouseover=this.stop() onmouseout=this.start() behavior="scroll" scrollamount="2" scrolldelay="0" loop="-1"  hspace="10" vspace="10">'; */
    		     			html += this.content;
    		     			html += '</li></ul></div>';
    		     		}
    		     	});
    		     	 html += "</ul>";
    		     	$(target).html(html);
    		     	var vheight = $(".list_lh ul li").height();
    		     	if(vheight > 300){
    		     		vheight = vheight - 30;
    		     	}else{
    		     		vheight = 300;
    		     	}
/*     				$("div.list_lh").myScroll({
    					speed:40, //数值越大，速度越慢
    				 	rowHeight:vheight //li的高度 
    				}); */
    		    }	
    			
    		});
	 }
</script>
