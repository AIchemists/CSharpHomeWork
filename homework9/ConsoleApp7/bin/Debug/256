       					url1 = data.data[0].picsrc.replace("/framework/ueditor/upload?","/framenet/ueditor/upload?");
       					$("#pic1").attr("src",url1);
       					$("#pic1").parent().attr("target","_blank");
       					$("#pic1").parent().attr("href",data.data[0].picurl);
