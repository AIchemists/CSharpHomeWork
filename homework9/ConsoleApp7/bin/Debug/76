												$("#sel_city").append(function() {
													return "<option value=" + item.cityname + ">" + item.cityname + "</option>";
												})
											})

											$.each(datalist[0].citydata, function(key, titleval) {
												$("#sel_title").append(function() {
													return "<option value=" + titleval.urls + ">" + titleval.title + "</option>";
												})
											})
											$("#downurl").attr("href", datalist[0].citydata[0].urls);
											var citydata = new Array();
											$("#sel_city").change(function() {
												var cityname = $(this).val();

												$.each(datalist, function(index, item) {
													if(cityname == item.cityname) {
														citydata = item.citydata;

													}
												})
												$("#sel_title").html("<option>-请选择-</option>");
												$.each(citydata, function(key, titleval) {
													$("#sel_title").append(function() {
														return "<option value=" + titleval.urls + ">" + titleval.title + "</option>";
													})
												})
											})

											$("#sel_title").change(function() {
												var downurl = $(this).val();

												$("#downurl").attr("href", downurl);
											})
