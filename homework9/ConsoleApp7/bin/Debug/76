						<script type="text/javascript">			
$(function() {
				$('.wy-more').click(function() {
					$('#wy-list li').hide()
					var arr = [];
				while(true) {
					var isExists = false;
					// 获取一个0-20范围的数
					var random = parseInt(30 * (Math.random()))
					// 判断当前随机数是否已经存在
					for(var i = 0; i < arr.length; i++) {
						if(random === arr[i]) {
							isExists = true;
							$('#wy-list li').eq(arr[i]).html()

							break;
						}

					}
					// 如果不存在，则添加进去
					if(!isExists)
						arr.push(random);
					//$('#wy-list li').eq(arr[i]).html()
					// 如果有10位随机数了，就跳出
					if(arr.length === 3)
						break;
				}
					console.log(arr)
				//console.log(1)
				for(var i=0;i<3;i++){
					$('#wy-list li').eq(arr[i]).show()
				}
				})
			})

						</script>
						
