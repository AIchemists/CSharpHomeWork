<script type="text/javascript">
/*
function: channel homepage
modified: 20120924
coding:austinjin
*/
var gBossZoneHandler = new bossZoneHandler();
var gImage;
var registerZone2 = gBossZoneHandler.registerZone2;

if(document.addEventListener)
    document.addEventListener("click", registerZone2, false);
else if(document.attachEvent)
    document.attachEvent("onclick", registerZone2);

function bossZoneHandler()
{
    var sFlag=false;
	this.registerZone2 = function (ev,clickType)
	{
	    var loopTryNum = 10;
	    var bossID = 1337; 
		  
	    try
		{		
		    //qq
			var a=document.cookie.match(new RegExp('(^|)o_cookie=([^;]*)(;|$)'));
			var iQQ=(a==null?"":unescape(a[2]));
			var purl='';
			var zoneId='';
			
			if(typeof clickType == 'undefined')
			{
			    if(sFlag) return true;
		        var ev = window.event || ev;
		        var et = ev.srcElement || ev.target;
			    var type=et.tagName;
		        if (type != "A" && type != "IMG" ) 
			    {
			        return true;
			    } 
			
			    if (type == "A")
			    {
			        purl = et.href;
			    }
			    else if (type == "IMG")
			    {
			        purl = et.parentNode.href;
			    }
			
			    //pseudo attr
			    for (var i=loopTryNum-1,tagNode=et;i>=0;i--,tagNode=tagNode.parentNode)
			    {
			        if(tagNode.attributes['bossZone'])
				    {
			            zoneId = tagNode.attributes['bossZone'].nodeValue;
				    } else if(tagNode.attributes['bosszone'])
				    {
				        zoneId = tagNode.attributes['bosszone'].nodeValue;
				    }
				    else if(tagNode.attributes['BossZone'])
				    {
				        zoneId = tagNode.attributes['BossZone'].nodeValue;
				    }
				    else if(tagNode.attributes['Bosszone'])
				    {
				        zoneId = tagNode.attributes['Bosszone'].nodeValue;
				    }

			        if(zoneId) break; 
			    }		
			    if(!zoneId) return;
			}
			else
			{
			    zoneId = ev.bossZone;
				purl = ev.url;
				sFlag = true;
				setTimeout(function (){sFlag = false;},200);
			}
			
			var localUrl = location.href;
			var site = localUrl.substring(7,localUrl.indexOf('.qq.com'));
			site = site.substr(site.lastIndexOf('.')+1);

			var iurl = '//btrace.qq.com/collect?sIp=&iQQ='+iQQ+'&sBiz=qq.com&sOp='+zoneId+'&iSta=&iTy='+bossID+'&iFlow=&sSite='+site+'&sUrl='+escape(purl)+'&sLocalUrl='+escape(location.href)+'&'+Math.random();
			gImage = new Image(1,1);
			gImage.src = iurl;
		} catch (e) {}
	}
}</script><!--[if !IE]>|xGv00|42da10df8dbe69f70bc888425a897314<![endif]-->
<script type="text/javascript">
/*
function: �����̻��ϱ�
modified: 20141023
author:quanyin@tencent.com
*/

function autoBussBoss(ev)
{
	
    var loopTryNum = 10;
    var autoBossID= 2586; 
    try
	{		
    	
		var a=document.cookie.match(new RegExp('(^|)o_cookie=([^;]*)(;|$)'));
		var iQQ=(a==null?"":unescape(a[2]));
		var purl='';
		var autoBussBoss = '';
        var ev = window.event || ev;
        var et = ev.srcElement || ev.target;
	    var type=et.tagName;
        if (type != "A" && type != "IMG" ) 
	    {
	        return true;
	    } 
	
	    if (type == "A")
	    {
	        purl = et.href;
	    }
	    else if (type == "IMG")
	    {
	        purl = et.parentNode.href;
	    }
	    
		for (var i=loopTryNum-1,tagNode=et;i>=0;i--,tagNode=tagNode.parentNode)
	    {
			autoBussBoss = tagNode.getAttribute('autobussboss');
	        if(autoBussBoss) break; 
	    }	
		
	    if(!autoBussBoss) return;
	
		var localUrl = location.href;
		if(autoBussBoss){

			autoBossImg = new Image();
			var autoBossData = autoBussBoss.split('|');

			var timestamp = Date.parse(new Date());

			var autourl = 'http://btrace.qq.com/collect?ftime=&sIp=&iQQ='+iQQ+'&sBiz=dealerExposure&sOp='+autoBossData[0]+'&iSta=&iTy='+autoBossID+'&iFlow='+timestamp+'&sUrl='+escape(localUrl)+'&sRef='+escape(purl)+'&sPageId='+autoBossData[2]+'&sPos='+autoBossData[1];
			autoBossImg.src = autourl;
		}


	} catch (e) {}
}

if(document.addEventListener)
	document.addEventListener("click", autoBussBoss, false);
else if(document.attachEvent)
	document.attachEvent("onclick", autoBussBoss);
