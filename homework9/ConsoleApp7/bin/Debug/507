<script type="text/javascript">
    function closeDiv(){
        $("#tips").hide();
    }


    function hideDiv(){
        $("#tipsClose").hide();
        $("#tipsOpen").show();
        $("#tipsBD").hide();
        $("#tips").css({"height":"35px"});
    }

    function showDiv(){
        $("#tipsClose").show();
        $("#tipsOpen").hide();
        $("#tipsBD").show();
        $("#tips").css({"height":"180px"});
    }
</script><!--[if !IE]>|xGv00|582715b27a730101772ab1452e99366f<![endif]-->
