<script>
    //json������ȡ
    function rankData2() {
        var html = "";
        jQuery.ajax({
            url: "//tags.webdev.com/interface/tag/articles.php?tag=%E7%94%A8%E8%BD%A6&oe=utf8&site=auto&p=1&l=12&platform=web",
            dataType: "jsonp",
            success: function (data) {
                var articles = data.data.articles,
                    html = '';

                for (var i = 0, len = 2; i < len; i++) {
                    var item = articles[i];

                    var title = item.title.length > 25 ? item.title.substr(0, 25) + '...' : item.title;

                    html += "<li><a href='" + item.url + "' target='_blank' title='" + item.title + "'>" + title + "</a></li>";

                }

                jQuery("#toutiao02_4_5").html(html);
                return true;
            }
        })
    }
    //rankData2();
</script><!--[if !IE]>|xGv00|680ecf709197ccca1a37181eb7da86ca<![endif]-->
