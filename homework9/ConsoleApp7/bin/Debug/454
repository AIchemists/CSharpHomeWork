    }
}
function g_ztb_logout_callback(){
    var houseTuanIdSpan = document.getElementById("houseTuanId").getElementsByTagName("span");
    for(var i=0; i<3; i++){
        var houseId = houseTuanIdSpan[i].innerHTML;
        var btnId = document.getElementById('houseTuan_' + houseId);
        if(btnId.innerHTML != '������'){
            btnId.innerHTML = '������';
            btnId.className = "fr"; 
            btnId.onclick = function(){
                window.g_kft_signup_house_id_ztb = houseId; kft_gb_house_signup(houseId, callback_ztb_house_signup_success);return false;
            };
        }
    }
}
