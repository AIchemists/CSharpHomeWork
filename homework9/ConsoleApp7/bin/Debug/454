function getLoginAfterData() {
    loadJson("//db.house.qq.com/index.php?mod=kanfang&act=usersignupinfo&callback=kft_my_info", "kft_my_info",
        function(data) {
            var html = template.render("kft_my_info", data);
            K("LoginAfterDataContent").html(html);
            if(data.docs[0].signup_route_num == "0" && data.docs[0].signup_group_num == "0" && data.docs[0].favorite_house_num == "0"){
                K("logined_1").hide();  
                K("login_0").show()
            }
        },"UTF-8");
};
var cityId = $("#cityid").val();
var cityJc = $("#cityjc").val();
function g_home_kft_login(){
    getLoginAfterData();
