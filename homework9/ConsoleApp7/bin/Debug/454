    },

    _url: '//apis.map.qq.com/ws/location/v1/ip?key=UCJBZ-SRHL5-VQHIC-QI3HH-7HRIK-XQF3W&callback=Ip_Where.Go&output=jsonp',

    Check: function () {
        var head = document.getElementsByTagName("head")[0];
        try {
            if (this.script) {
                head.removeChild(this.script);
            }
        } catch (e) {}
        this.script = document.createElement("script");
        this.script.setAttribute('src', this._url);
        this.script.type = "text/javascript";
        head.appendChild(this.script);
    },

    Go: function (ip_info) {
        //console.log(ip_info);
        var ad_info = null;
        if (ip_info && ip_info.result) {
            ad_info = ip_info.result.ad_info;
        }
        if (!ad_info) {
            return;
        }

        //��city��province˳����
        this.go_url = '';

        if (ad_info["city"] && this._map[ad_info["city"]]) {
            this.go_url = this._map[ad_info["city"]];
        } else if (ad_info["province"] && this._map[ad_info["province"]]) {
            this.go_url = this._map[ad_info["province"]];
        }

        var referrer = document.referrer;
        var refDomain = '';
        if (referrer) {
            var match = referrer.match(/\/\/([^\/]+)/);
            refDomain = match && match[1] || '';
        }

        if (this.go_url) {
            //��ֹѭ����ת
            if (refDomain && this.go_url.indexOf(refDomain) > 0) {
                return;
            }
            //alert(this.go_url);
            window.location.href = this.go_url;
        }
    }

};

if (window.location.href.indexOf('bj.house.qq.com') == -1) {
    Ip_Where.Check();
}
