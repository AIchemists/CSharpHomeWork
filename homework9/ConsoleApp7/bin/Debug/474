<script type="text/javascript">
function changeSecretVctoriaWidth(){
	var sreenWidth = $(window).width();
	if(sreenWidth >= 1280){
		$("#secret_vctoria_wrap").addClass("secret_vctoria_wrap1240");
	}
	else{
		jQuery("#secret_vctoria_wrap").removeClass("secret_vctoria_wrap1240");
	}
}
changeSecretVctoriaWidth();
window.onresize=function(){
	changeSecretVctoriaWidth();
}
</script><!--[if !IE]>|xGv00|de8694d70741b61a676cce2474b3c276<![endif]-->
