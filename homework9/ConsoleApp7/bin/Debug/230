    $(window).scroll(function () {
        var o = $(window).scrollTop(), s = $("#top_bar") ,j = $("#fixed_left_box"), t = $('.logo');
        if (o >= 55) {
            s.addClass("fiexd");
            j.addClass("on");
            t.hide();
        } else {
            s.removeClass("fiexd");
            j.removeClass("on");
            t.show()
        }
    });

