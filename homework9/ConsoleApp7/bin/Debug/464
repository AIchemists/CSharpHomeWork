	<script>
	
		var cancleSW = true;
		if('serviceWorker' in navigator && !cancleSW) {
			navigator.serviceWorker  
			  .register('service-worker.js')  
			  .then(function() { console.log('Service Worker Registered'); })
			  .catch(function(err){
			    console.log(err);
			  });
		}else if(cancleSW){
			serviceAble = false;
			navigator.serviceWorker
			  .getRegistration()
			  .then(function(res){
			    console.log(res)
			    if(res){
			      return res.unregister()
			    }else{
			      return 
			    }
			  }).then(function(d){
			    console.log(d);
			  });
		}else{
			serviceAble = false;
		}
		//tab�л�
		var lock = false;
		//�������ť�л��ײ��Ʋ���ʾ���
		var isCal = true; //�Ƿ���ʾ�Ʋ�͵���

		var timeEnd = new Date(),
			timeLoad = null,
			speedLoop = null;
    		serviceAble = true;

		function speedReport() {
			speedLoop && clearInterval(speedLoop);
			if (performance.timing) {
			  var getState = localStorage.getItem('performance');
			  var pageNum = 1;
			  if (!getState) {
			    if(!serviceAble){
			      pageNum = 3;
			    }else{
			      pageNum = 10;
			    }
			    localStorage.setItem('performance', 'true');
			  }else{
			    if(serviceAble){
			      pageNum = 2;
			    }
			  }
			  var url = "https://report.huatuo.qq.com/report.cgi?appid=20561&platform=pc&speedparams=",
			    parms = "flag1=22243&flag2=1&flag3="+pageNum+"&flag5=1",
			    time = performance.timing;
			  var t1 = timeStart - time.navigationStart,
			    t2 = timeEnd - time.navigationStart,
			    t5 = timeLoad - time.navigationStart,
			    t6 = time.loadEventEnd - time.navigationStart,
			    t7 = time.domComplete - time.navigationStart;
			  parms += "&1=" + t1 + "&2=" + t2 + "&5=" + t5 + "&6=" + t6 + "&7=" + t7;
			  if (performance.getEntriesByName("first-paint")[0]) {
			    t3 = performance.getEntriesByName("first-paint")[0].startTime,
			      t4 = performance.getEntriesByName("first-contentful-paint")[0].startTime;
			    parms += "&3=" + parseInt(t3) + "&4=" + parseInt(t4);
			  }
			  var reportImg = new Image();
			  reportImg.src = url + encodeURIComponent(parms);
			  PTTSendClick('speed', 'pable', '���ٳɹ�');
			}
		}
		// ��������
		function openDialog(e) {
			$('#' + e).addClass('on');
			showDialog.show(e);
		}

		function closeDialog() {
			showDialog.hide();
		}

		//��Ѷ��Ƶ����
		function videoPlay(boxid, vid, flag) {
			var player = new Txplayer({
				containerId: boxid,
				vid: vid,
				width: '887',
				height: '500',
				autoplay: true, //�Ƿ��Զ�����
				vodFlashExtVars: {
					share: 0,
					favorite: 0,
					showend: 1
				}
			});
		}

		function play(vid) {
			openDialog('vidoe-mod');
		    if(typeof Txplayer == "undefined"){
		      loadjs('//vm.gtimg.cn/tencentvideo/txp/js/txplayer.js',function(){
				videoPlay('video-player', vid, true);
		      })      
		    }else{
		      videoPlay('video-player', vid, true);    
		    }
		}
