    window.V_PAGE_INFO = {
        page: 'child',
        adType: 'index',
        channel: 'child'
    }
    var doc_el = document.compatMode == 'BackCompat' ? document.body : document.documentElement;
    txv.common.initPage({
        boss: {
            app: '频道页',
            page: '少儿频道',
            isRecalc: true
        },
        lazyload: {
            getScreenShowHeight: (function (doc) {
                return function () {
                    var scrollTop = window.pageYOffset || doc_el.scrollTop;
                    return doc.clientHeight + scrollTop + 600;
                }
            })(doc_el)
        }
    });
</script>
