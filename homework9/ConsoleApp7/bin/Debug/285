//�����������ϱ�
if(document.addEventListener){
    document.addEventListener("click", registerZone2, false);
}else if(document.attachEvent){
    document.attachEvent("onclick", registerZone2);
}else{
    document.onclick = registerZone2;
}
    
var gImage;
//����ϱ�
function registerZone2(param,clickType){
    var loopTryNum = 10;//���ϲ�ѯ����
    var loop=4;//��ǩ���ϲ�ѯ�ļ���
    var bossID = 3177; //�ϱ�ID��ͨ���̶�
    var pwd='763290108';//��bossId��Ӧ
    var pageType=bossData && bossData.pageType || '';
    var vertical=bossData && bossData.vertical || '';
    var purl='';
    var zoneId='';
    try{
        var num=document.cookie.match(new RegExp('(^|)o_cookie=([^;]*)(;|$)'));
        var iQQ=(num==null?"":unescape(num[2]));
        if(typeof clickType == 'undefined'){//ҳ��ṹ�ϼ�bosszone�����ϱ�
            var ev = window.event || param;
            var et = ev.srcElement || ev.target;
            //���ϲ���A��ǩ
            while(loop-- > 0){
                if("A"==et.tagName || "BODY"==et.tagName){
                    break;
                }else{
                    et = et.parentNode;
                }
            }
            var type=et.tagName;
            if (type != "A") {return;}
            //��ȡ�������
            purl = et.href;
            for(var i=loopTryNum-1,tagNode=et;i>=0;i--,tagNode=tagNode.parentNode){
                if(tagNode.attributes['bossZone']){
                    zoneId = tagNode.attributes['bossZone'].nodeValue;
                }else if(tagNode.attributes['bosszone']){
                    zoneId = tagNode.attributes['bosszone'].nodeValue;
                }else if(tagNode.attributes['BossZone']){
                    zoneId = tagNode.attributes['BossZone'].nodeValue;
                }else if(tagNode.attributes['Bosszone']){
                    zoneId = tagNode.attributes['Bosszone'].nodeValue;
                }
                if(zoneId) break; 
            }
            if(!zoneId){return;}
            
        }else{//js�ֶ������ϱ�
            zoneId = param.bossZone;
            purl = param.url || '';
        }
        var localUrl = location.href;//��ǰҳ��url
        var iurl = '//btrace.qq.com/kvcollect?BossId='+bossID+'&Pwd='+pwd+'&sOp='+zoneId+'&iQQ='+iQQ+'&pageType='+pageType+'&vertical='+vertical+'&sIp=&ftime=&sUrl='+escape(purl)+'&sLocUrl='+escape(location.href)+'&ran='+Math.random();
        gImage = new Image(1,1);
        gImage.src = iurl;

    }catch(e){}
    
}

//�ع��ϱ�
function ExposureBoss(id,name){
    var bossID = 3175; //�ϱ�ID��ͨ���̶�
    var pwd='1745364445';//��bossId��Ӧ
    var pageType=bossData && bossData.pageType || '';
    var vertical=bossData && bossData.vertical || '';
    
    try{
        var num = document.cookie.match(new RegExp('(^|)o_cookie=([^;]*)(;|$)'));
        var iQQ = (num==null?Math.floor(Math.random()*10000000000000000):unescape(num[2]));
        var iurl = '//btrace.qq.com/kvcollect?BossId='+bossID+'&Pwd='+pwd+'&sOp='+name+'&iQQ='+iQQ+'&pageType='+pageType+'&vertical='+vertical+'&sIp=&ftime=&sUrl='+escape(location.href)+'&ran='+Math.random();
        gImage = new Image(1,1);
        gImage.src = iurl;
        
    }catch(e){}

}
