	</script>						<script data-content="errorLoad">
    (function(){
		/*
		 1.通过seajs request劫持请求过程
		 2.换域名重试加载
		 3.sso通道重试加载
		 
		 */
        var retryMap = {};

        seajs.on('request', function(data) {
            var uri = data.uri;
            var mod = seajs.cache[uri];
            var dateFrom = +new Date();

            if(mod) {
                var cb = data.onRequest || function() {};
                data.onRequest = function(e) {
                    var ars = arguments;
                    var delay = +new Date() - dateFrom;
                    if(!e) return cb.apply(window, ars);//以防万一

                    if(e.type === 'error') {
                        if(retryMap[uri]) {
                            reportLoadInfo(delay, data.requestUri || uri, false, 'retry');//重试失败
                            if(window.HYB && window.mqq && HYB.browserInfo && ['qq', 'qzone'].indexOf(HYB.browserInfo.client) !== -1) {
                                var wnsRetryTimeout = 3000;
                                var wnsRetryTimer = setTimeout(function() {
                                    cb.apply(window, ars);
                                }, wnsRetryTimeout);
                                var wnsDateFrom = +new Date();
                                var wnsUrl = uri.indexOf('proxy/domain') === -1 ? fixPath(uri) : uri;
                                mqq.invoke('QzoneData', 'wnsCgi', {
                                    method: 'GET',
                                    url: wnsUrl,
                                    body: {},
                                    timeout: wnsRetryTimeout,
                                    dataNeedBase64: false
                                }, function(ret) {
                                    clearTimeout(wnsRetryTimer)
                                    delay = +new Date() - wnsDateFrom;

                                    if(delay < 3000 && ret && ret.status == 4 && ret.code == 0 && ret.data) {
                                        try {
                                            ret.data = ret.data.replace('define([', 'define("' + uri + '", [');
                                            new Function(ret.data)();
                                            cb.apply(window, ars);
                                            reportLoadInfo(delay, wnsUrl, true, false, 'wnsretry');//wns重试成功
                                        } catch (error) {
                                            reportLoadInfo(delay, wnsUrl, false, false, 'wnsretry');//wns重试失败
                                        }
                                    } else {
                                        reportLoadInfo(delay, wnsUrl, false, false, 'wnsretry');//wns重试失败
                                    }
                                })
                            } else {
                                cb.apply(window, ars);
                            }
                        } else {
                            if(ignoreErrorLoad(uri)) {
                                cb.apply(window, ars);
                            } else {
                                var retryUri = seajs.resolve(fixPath(uri));
                                retryMap[uri] = retryUri;
                                retryMap[retryUri] = true;
                                seajs.use(retryUri, function() {
                                    if(seajs.cache[retryUri]) {
                                        seajs.cache[uri].dependencies = seajs.cache[retryUri].dependencies;
                                        seajs.cache[uri].exports = seajs.cache[retryUri].exports;
                                    }
                                    cb.apply(window, ars);
                                });
                            }
                            reportLoadInfo(delay, uri, false);//加载失败
                        }
                    } else {
                        cb.apply(window, ars);
                        if(retryMap[uri]) {
                            reportLoadInfo(delay,data.requestUri || uri, true, 'retry');//重试成功
                        } else {
                            reportLoadInfo(delay, uri, true);//加载成功
                        }
                    }
                };
            }
        });

		/*
		 上报成功率到habo
		 成功抽样10%
		 失败报全量
		 */
        function reportLoadInfo(delay, uri, done, retry, wnsRetry) {
            if(done && (retry || wnsRetry) || !done) {
                console.log('SEAJSLOAD --- ' + (wnsRetry ? 'wns重试' : retry ? '重试' : '加载') + (done ? '成功' : '失败') + ': ' + uri);
                window.logger && window.logger.log('SEAJSLOAD --- ' + (wnsRetry ? 'wns重试' : retry ? '重试' : '加载') + (done ? '成功' : '失败') + ': ' + uri + ';delay:' + delay);
            }
			
			if(done && Math.random() > 0.1){
				return;
			}

            if(wnsRetry) {
                if(done) {
                    reportHaboGlobal( 0,'1000369', '/jswnsreload/all');
                } else {
                    reportHaboGlobal( -1,'1000369', '/jswnsreload/all');
                }

                sendLog('/jsload/error/weishi_pc', 'type: wnsreload ' + (done ? 'succ' : 'fail') + '. url:' + uri + ';');
            } else if(retry) {
                if(done) {
                    reportHaboGlobal( 0,'1000369', '/jsreload/all');
                } else {
                    reportHaboGlobal( -1,'1000369', '/jsreload/all');
                }

                sendLog('/jsload/error/weishi_pc', 'type: reload ' + (done ? 'succ' : 'fail') + '. url:' + uri + ';');
            } else {
                if(done) {
                    window.logger && window.logger.log('SEAJSLOAD --- 加载成功: ' + uri + ';delay:' + delay);
                    reportHaboGlobal( 0 ,'1000369', '/jsload/all');
                } else {
                    window.logger && window.logger.log('SEAJSLOAD --- 加载失败: ' + uri + ';delay:' + delay);
					
                    reportHaboGlobal( -1 ,'1000369', '/jsload/all');
					
                    reportDomainToIp(getDomain(uri).hostname);
                    sendLog('/jsload/error/weishi_pc', 'type: load. url:' + uri + ';');
                }
            }
        }

        function fixPath(path) {
            path = path.replace(/\.\d{13}\./,'.').replace(/_offline=1&?/, '');

            if(path.indexOf('proxy/domain') === -1) {
                path = path.replace('//', '//h5.qzone.qq.com/proxy/domain/');
                if(path.indexOf('?') !== -1) {
                    path += '&_qzhw_channel=wns';
                } else {
                    path += '?_qzhw_channel=wns';
                }
            } else {
                path = path.replace('h5.qzone.qq.com/proxy/domain/', '');
            }

            return path;
        }
        function ignoreErrorLoad(url){
            return url.indexOf('.css') !== -1 || url.indexOf('ignoreErrorLoad') !== -1
        }

        function sendLog(path, msg) {
            var xhr = new XMLHttpRequest();
            xhr.open('post', 'https://h5.qzone.qq.com/log/post' + path, true);
            xhr.withCredentials = true;
            xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
            xhr.send(msg);
        }
        function getDomain(url){
            var a = document.createElement('a');
            a.href = url;
            return {
                hostname : a.hostname
            };
        }
        function reportDomainToIp(domain){
            if (!/QQ\/([\d\.]+)/.test(navigator.userAgent)) {
                return ;
            }
            window.getHostIpAddressCallback = function(result){
                if(result.code == 0 && result.host_ip){
                    sendLog('/jsload/error/weishi_pc', domain + ':' + result.host_ip);
                }
            };
            new Image().src = 'jsbridge://Qzone/getHostIpAddress?p=%7B%22host%22%3A%22' + domain + '%22%2C%22callback%22%3A%22getHostIpAddressCallback%22%7D';
        }
    }());
</script>						<!--wnsdiff-seajsconfig--><script data-content="seajsConfig">
