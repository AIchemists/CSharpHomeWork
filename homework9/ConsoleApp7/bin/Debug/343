    window.isH5Test = false;        window.fixPicUrl = function fixPicUrlFun(url) {
    if (!url) {
        return '';
    }
    if (url.match(/^(http:)?\/\/qzonestyle/)) {
        return url.replace(/^http:/, '');
    } else if (url.match(/^(http:)?\/\/imgcache\.qq\.com\//)) {
        return url.replace(/^http:/, '');
    } else if (url.match(/^(http:)?\/\/q.qlogo.cn\//)) {
        return url.replace(/^http:/, '');
    } else if (url.match(/^http:\/\/i.gtimg.cn/)) {
        return url.replace(/^http:/, '');
    } else if (url.match(/^http:\/\/(.*).qpic.cn\//)) {
        return url.replace(/^http:/, '');
    } else if (url.match(/^http:\/\/a[0-9].qpic.cn\//)) {
        return url.replace(/^http:/, '');
    } else if (url.match(/^(http:)?\/\/qlogo[0-9].store.qq.com\//)) {
        return url.replace(/^http:/, '');
    } else if (url.match(/^http:\/\/(.*).photo.store.qq.com\//)) {
        return url.replace(/^http:/, '');
    } else if (url.match(/^http:\/\/group.store.qq.com\//)) {
        return url.replace(/^http:/, '');
    } else if (url.match(/^http:\/\//) && !/^http:\/\/proxy\.gtimg\.cn\/tx_tls_gate=/.test(url)) {
        return '//proxy.gtimg.cn/tx_tls_gate=' + url.replace(/^http:\/\//, '');
    } else if (url.match(/^\/\//) && !/^\/\/proxy\.gtimg\.cn\/tx_tls_gate=/.test(url)) {
        return '//proxy.gtimg.cn/tx_tls_gate=' + url.replace(/^\/\//, '');
    } else {
        return url;
    }
