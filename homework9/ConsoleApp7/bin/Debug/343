			(function() {
				if (navigator.userAgent.match(/(iPhone)/)) {
					if ((screen.height == 812) && (screen.width == 375)) {
							document.body.scrollTop=0
							console.log('This is iphoneX');
					} else {
						console.log('This is not iphoneX');
					}
				}

				function pgvGetUserInfo() {
					var m = document.cookie.match(/(^|;|\s)*pvid=([^;]*)(;|$)/);
					var pvid;
					if (m && m.length > 2) {
						pvid = m[2];
					} else {
						pvid = (Math.round(Math.random() * 2147483647) * (new Date().getUTCMilliseconds())) % 10000000000;
						document.cookie = 'pvid=' + pvid + '; path=/; domain=qq.com; expires=Sun, 18 Jan 2038 00:00:00 GMT;';
					}
					return '&pvid=' + pvid;
				}
				var reportPV = function(domain, pathname, opt) {
					domain = domain || 'h5.qzone.qq.com';
					pathname = pathname || location.pathname.substring(1).replace('/', '.');


					var referURL = document.referrer || '';
					var referDomain = domain;

					new Image().src = "https://h5.qzone.qq.com/proxy/domain/pingfore.qq.com/pingd?dm=" +
						domain + "&url=" + pathname + "&tt=-&rdm=" + referDomain + "&rurl=" + referURL + pgvGetUserInfo() +
						"&scr=-&scl=-&lang=-&java=1&cc=-&pf=-&tz=-8&ct=-&vs=3.3&emu=" + Math.random();
				};
				var pvPath = (function() {
					var pathName = location.pathname;
					if (!pathName || pathName.length <= 0) {
						pathName = location.hostname;
						return pathName;
					}
					if(pathName.match(/^\/weishi\//)){
						pathName = pathName.replace(/\/\w{14,1000}/g, '/_');
					}
					pathName = pathName.substr(1).replace(/(qzone\/hybrid\/(module\/)?|\.html|\/?\d{5,1000})/g, '');
					return pathName.replace(/\//ig, '.');
				})();

				var ua = navigator.userAgent || "";
				var isWX = ua.match(/MicroMessenger\/([\d\.]+)/),
					isQQ = ua.match(/QQ\/([\d\.]+)/),
					isQzone = ua.match("Qzone"),
					isWS = ua.match("_WEISHI_");

				if(Math.random() < 0.01){
					if(isQQ){
						pvPath = "SyncStatQQ." + pvPath;
						reportPV("h5.qzone.qq.com", pvPath);

					}else if(isWS){
						pvPath = "SyncStatWS." + pvPath;
						reportPV("h5.qzone.qq.com", pvPath);
					}
				}
			})();
		</script>

