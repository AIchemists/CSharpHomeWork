        var lastArr = new Array();
        (function () {
            function last() {
                $.ajax({
                    type: 'GET',
                    async: false,
                    url: '//hdl.qq.com/zlkdatasys/data/ingamewqnew.json',
                    dataType: 'jsonp',
                    jsonpCallback: 'getData',
                    success: function (data) {
                        console.log(data);
                        var overArr = data.wq_b0;
                        lastArr = JSON.parse(JSON.stringify(overArr));
                        DataAfferent()
                    },
                    error: function (data) {
                        console.log(data);
                    }
                })
            }
            last();
            function DataAfferent() {
                var allArr = new Array();
                for (var i = 0; i < lastArr.length; i++) {
                    for (var j = 0; j < lastArr[i].wqxq_70.length; j++) {
                        allArr.push(lastArr[i].wqxq_70[j]);
                    }
                }
                allArr = bubbleSort(allArr);
                var all = {
                    wqfl_eb: 'ȫ��',
                    wqflid_2b: "",
                    wqxq_70: allArr
                };
                lastArr.unshift(all);
                for (var i = 0; i < lastArr.length; i++) {
                    lastArr[i].wqxq_70 = bubbleSort(lastArr[i].wqxq_70);
                }
                var html = new String();
                for (var i = 0; i < lastArr.length; i++) {
                    html += '<li><a href = "javascript:">' + lastArr[i].wqfl_eb + '</li>'
                }
                $('#ct-tab').html(html);
                $('#ct-tab li').eq(0).addClass('on');
                gethtml(0);
            }
            $('#ct-tab').on('click', 'li', function () {
                $(this).addClass('on').siblings('li').removeClass('on');
                gethtml($(this).index());
            })

            function bubbleSort(arr) {
                var len = arr.length;
                for (var i = 0; i < len; i++) {
                    for (var j = 0; j < len - 1 - i; j++) {
                        if (arr[j].wqdjwz_b2 > arr[j + 1].wqdjwz_b2) {
                            var temp = arr[j + 1];
                            arr[j + 1] = arr[j];
                            arr[j] = temp;
                        }
                    }
                }
                return arr;
            }

            function gethtml(num) {
                var num = num ? num : 0;
                var html = '';
                // console.log(num);
                // console.log(lastArr);
                var showType = 8;
                for (var i = 0; i < lastArr[num].wqxq_70.length; i++) {
                    if(i < showType){
                        html += '<li>' +
                        '<a href="javscript:" onclick="PTTSendClick(' + "'btn','pop1','��������'" + ')">' +
                        '<img src="' + lastArr[num].wqxq_70[i].wqtp_89 + '" width="290" height="139" >' +
                        '<strong class="mask"></strong>' +
                        '<img src="' + lastArr[num].wqxq_70[i].wqdj_52 + '" alt="" class="level">' +
                        '<span class="jx' + lastArr[num].wqxq_70[i].wqjxqk_3d + '"></span>' +
                        '<p class="name">' + lastArr[num].wqxq_70[i].wqmc_ba + '</p>' +
                        '</a>' +
                        '</li>';
                    }
                    
                }
                $('#wqlist').html(html);
            }
            $('#wqlist').on('click', 'li', function () {
                var num = null;
                for (var i = 0; i < $('#ct-tab li').length; i++) {
                    if ($('#ct-tab li').eq(i).hasClass('on')) {
                        num = i;
                        break;
                    }
                }
                getpop(num, $(this).index());
            })
        })();

