                        $("div.x-list p.on").removeClass("on");
                        $(this).addClass("on");
                        $("div.x-box div.x-new:visible").hide();
                        $("div.x-box div.x-new:eq(" + index + ")").show();
                    })
                })
            })
        })(jQuery);

        // �ַ���
        $(function () {
            var $centerwell_first = $('#centerwell li:first');
            $centerwell_first.animate({
                width: '873px'
            }, 300);
            $centerwell_first.find('h3').addClass("on");

            $('#centerwell li').click(function () {
                if (!$(this).is(':animated')) {
                    $(this).animate({
                        width: '873px'
                    }, 300).siblings().animate({
                        width: '162px'
                    }, 300);

                    $('#centerwell li h3').removeClass("on");
                    $(this).find("h3").addClass("on");
                }
            });
        });


        // ����
        function TGDialogS(e) {
            showDialog.show(e);
        }

        function closeDialog() {
            showDialog.hide();
        }
        // �ർ��
        var sideNav = new Scroller({
            navId: "side-nav", //����ID
            navs: "navs", //������ǩ, ȡֵΪ��ǩ������class��
            conts: ["top", "cont01", "cont02", "cont07", "cont03", "cont05", "cont08"], //���ݰ��ID
            cls: "curr", //����ѡ��class
            drag: true //�Ƿ����϶�
        });

        function g(obj) {
            return (typeof obj == 'object') ? obj : document.getElementById(obj);
        }

        function settab(a, b) {
            var tbs = g(a).getElementsByTagName("li"),
                lys = new Array();
            for (var i = 1; i <= tbs.length; i++) {
                var lay = g(b + i),
                    tb = tbs[i - 1];
                lys.push(lay);
                change(tb, lay);
            };

            function change(x, y) {
                x.onclick = function () {
                    if (y.style.display == "block") return;
                    else {
                        for (var j = 0; j < lys.length; j++) {
                            lys[j].style.display = "none";
                            tbs[j].className = tbs[j].className.replace(new RegExp("\\b" + "on" + "\\b\\s*", "g"),
                                "");
                        };
                        x.className += " " + "on";
                        y.style.display = "block";
                    }
                };
            }
        }
        // settab("tbb", "bt");
        settab("zt-tab", "zt-ct");
        settab("ss-tab", "ss-ct");

        //��Ƶ
        function playVideo(vid) {
            var video = new tvp.VideoInfo();
            video.setVid(vid);
            var player = new tvp.Player('100%', '100%');
            player.setCurVideo(video);
            player.addParam("autoplay", "1");
            player.addParam("wmode", "opaque");
            player.addParam("flashskin", "//imgcache.qq.com/minivideo_v1/vd/res/skins/TencentPlayerMiniSkin.swf");
            player.write("video");
        }

        function popVideo(vid) {
            TGDialogS('pop-video')
            playVideo(vid);
        }

        function hideVideo() {
            var flv_s = document.getElementById("video");
            if (flv_s) {
                flv_s.innerHTML = "";
            }
            closeDialog();
        }

        //��������
        $(window).bind("scroll resize", function () {
            var winWidth = $(window).width();
            var winHeight = $(window).height();
            if (winWidth > 1250 && $(window).scrollTop() > 500) {
                $('.side-nav').css('visibility', 'visible');
            } else {
                $('.side-nav').css('visibility', 'hidden');
            }
        });
        //��Ϸ��ɫ
        $(function () {
            $('#slides').slides({
                generatePagination: false,
                play: 3000,
                pause: 1500,
                effect: 'slide',
                hoverPause: true
            });
        });
