            }
            else {
                $(".head_pic img").attr("src", "//img1.gtimg.com/kid/pics/hv1/155/153/1753/114027995.png");
            };

            if (data.user.nickname) 
            {
                $(".has_works .name").text(data.user.nickname);
            }
            else {
                $(".has_works .name").text(QQ.localData.get('nick'));
            };

            if(data.work_num.length>0){
                for (var i = 0; i < data.work_num.length; i++) {
                    if(data.work_num[i].maintype==1){
                        $("#draw_num").text(data.work_num[i].num);
                    }
                    else if(data.work_num[i].maintype==2){
                        $("#cart_num").text(data.work_num[i].num);
                    }
                    else if(data.work_num[i].maintype==3){
                        $("#fun_num").text(data.work_num[i].num);
                    }
                };
            }
        },
        error: function() {
            alert("��������");
        }
    });
