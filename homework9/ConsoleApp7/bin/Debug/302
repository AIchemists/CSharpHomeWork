	<script src='./images/common_2019110.js'></script>
    <script src="./images/swiper_20171211.min.js"></script>
    <script>
    	    jQuery.support.cors = true; 
			$.ajax({
				type : "GET",
				dataType : "JSON",
				url : "https://msjw.ga.sz.gov.cn/wwyapi/externalNewsApi/getPortalNewsByRegion.html",
				data : "source=SZGAMH&sign=fc7687422ba7d1476b734bbfa38c6e1e&region=b&random="+ Math.random(),
				success: function(msg){
					if(msg.code="0000")
					{
					       $("#rmzx").html("");
					       var tmp = "";
                                                
                                               for(var i=0; i < 4; i++)
						{      
							tmp += "<li>";
							tmp += "<a href=\""+ msg.data.newsList[i].newsUrl +"\" target=\"_blank\">"+ msg.data.newsList[i].newsTitle +"</a><span>"+ msg.data.newsList[i].newsDate.split(" ")[0] +"</span>";
							tmp += "</li>";
                                                        
						}

						$("#rmzx").append(tmp);
						$("#rmzx").append(" <li class=\"more-a\"><a href=\"https://msjw.ga.sz.gov.cn/wwyjfzx/\" target=\"_blank\">更多融媒资讯 ></a></li>");
					}
					else
					{
						$("#rmzx").html("<li>"+ msg.msg +"</li>");
					}
				}
			});

			$.ajax({
				type : "GET",
				dataType : "JSON",
				url : "https://msjw.ga.sz.gov.cn/wwyapi/externalNewsApi/getPortalNewsByRegion.html",
				data : "source=SZGAMH&sign=fc7687422ba7d1476b734bbfa38c6e1e&region=c&pageSize=5&random="+ Math.random(),
				success: function(msg){
					if(msg.code="0000")
					{
						$(".list-ul").html("");
						var tmp = "";
						
						var count = 5;
						
						if(count<5)
							count = msg.data.newsList.Count; 
						
						for(var i=0; i < count; i++)
						{
							tmp += "<li><a href=\""+ msg.data.newsList[i].newsUrl +"\" target=\"_blank\">"+ (msg.data.newsList[i].newsTitle.length>24? msg.data.newsList[i].newsTitle.substring(0,24) + "...":msg.data.newsList[i].newsTitle) +"</a><span>" + msg.data.newsList[i].newsDate.split(" ")[0] + "</span></li>";
						}

						$(".list-ul").append(tmp);
                        
					}
					else
					{
						$(".list-ul").html("<li>"+ msg.msg +"</li>");
					}
				} 
			});
    </script>
    <script type="text/javascript">
    	//顶部公告
