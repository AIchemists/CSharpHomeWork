<script>
(function(w){
	document.domain='qq.com';
	var targetWebQQ = {
		stopBubble:function(e){
			if (e && e.stopPropagation)
				e.stopPropagation();
			else
				w.event.cancelBubble = true;
		},
		stopDefault:function(e){
			if (e && e.preventDefault)
				e.preventDefault();
			 else
				w.event.returnValue = false;
				return false;
		},
		clickWebQQ:function(ev){
			var org = ev;
			ev=ev||w.event;
			obj=ev.srcElement?event.srcElement:ev.target;
			if(obj.tagName == "A"||obj.tagName == "IMG"){
				if(obj.tagName == "IMG"){
					parent.qqweb.portal.openInWebBrowser({url:obj.parentNode.href,isHideBar:false,title:obj.parentNode.alt});
				}else{
					parent.qqweb.portal.openInWebBrowser({url:obj.href,isHideBar:false,title:obj.innerHTML});
				}
				targetWebQQ.stopBubble(org);
				targetWebQQ.stopDefault(org);
				return false;
			}
		},
		init:function(){
			if(document.addEventListener)
				document.addEventListener("click", targetWebQQ.clickWebQQ, false);
			else if(document.attachEvent)
				document.attachEvent("onclick", targetWebQQ.clickWebQQ);
		}
	};
	if(self!=top){
		targetWebQQ.init();
	}
})(window)
