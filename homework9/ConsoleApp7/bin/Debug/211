<script type="text/javascript">
function getid(div){
	return document.getElementById(div);
}

function option_display(n,start,selected){
	for(i=start; i<=n; i++){
		var date = i;
		if(date<10&&n!=15&&n!=6&&n!=180&&n!=90){date="0"+date;}
		if(date==selected){
			document.write("<option value="+date+" selected>"+date+"</option>");
		}else{
			document.write("<option value="+date+">"+date+"</option>");
		}
	}
}

function changeday(){
	var y = getid("81pan_year");
	var m = getid("81pan_month");
	var d = getid("81pan_day");
	var s_y = Number(y.value);
	var s_m = Number(m.value);
	var s_d = Number(d.value);
	if(((s_y%4==0&&s_y%100!=0)||s_y%400==0)&&s_m==2){
		option_change("29");
	}else{
	  if(s_m==4||s_m==6||s_m==9||s_m==11){option_change("30");}
	  else if(s_m==2){if(d.selectedIndex==28){d.selectedIndex=0;};option_change("28");}
	  else{option_change("31");}
	}
}

function option_change(n){
	var d = getid("81pan_day");
	var d_index = d.selectedIndex;
	d.remove(30);
	d.remove(29);
	d.remove(28);
	for(i=29;i<=n;i++){d.options.add(new Option(i,i));}
	if(d_index>=28 && d_index<=n-1){d.selectedIndex = d_index;}
}

function draw(){
	var form = getid('81pan_form');
	//if(form.yname.value==''){alert('���������ǳƣ�');return false;}
	//getswf('xingpan').js_draw(type,m,d,y,h,i,t,p);
	var sex = 0;
	if(form.sex[0].checked===true){sex=0;}else{sex=1;}
	if(p_data == new Array(form.month.value,form.day.value,form.year.value,form.hour.value,form.minute.value,form.city.value,form.province.value,form.yname.value,sex).toString()){alert('���������Ѿ�������ϣ��޸�������Ϣ����ܻ����µ�����Ŷ��#���̽��#');return false;};
	getswf('xingpan').js_transit('draw',form.month.value,form.day.value,form.year.value,form.hour.value,form.minute.value,form.city.value,form.province.value,form.yname.value,sex);
	p_data = new Array(form.month.value,form.day.value,form.year.value,form.hour.value,form.minute.value,form.city.value,form.province.value,form.yname.value,sex).toString();
	isdraw = true;
}

function getswf(name){
	var swf = document.getElementById(name);
	if(navigator.appName.indexOf('Microsoft')==-1){swf = swf.getElementsByTagName('embed')[0];}
	return swf;
}

var isdraw=false;
var p_data;
var action_url = "http://www.81pan.com/qqresult.php";
function draw_id(id){
	if(id>0){p_id = id;}
}

function detail(){
	var url;
	var form = getid('81pan_form');
	if(form.yname.value==''){alert('���������ǳƣ�');return false;}
	form.action = action_url;
	form.submit();
}

function share(){
	var form = getid('81pan_form');
	var at = "(������@qqastro)";
	var title;
	var pic;
    var url;
	if(isdraw==false){
		url = "http://astro.fashion.qq.com/xingpan/index.htm";
		pic = '';
		title = "#���̽��#����ǿ���Flash�������̣������ص������������Ѷ����ר��Ϊ����ҷ���Ŷ������ʲô����@���ǰɡ���"+at;;
		var key = "800100263";
		var share_url = "http://share.v.t.qq.com/index.php?c=share&a=index&title="+encodeURIComponent(title)+"&url="+url+"&appkey="+key+"&pic="+pic;
		window.open(share_url);
	}else{
		if(form.yname.value==''){alert('���������ǳƣ�');return false;}
		var rand = 'qq'+Math.floor(100000 * Math.random());
		getswf('xingpan').js_upload(rand);
		form.action = action_url + "?op=share&pic_id="+rand;
		form.submit();
	}

}

function check_user(){
	var ua = navigator.userAgent.toLowerCase();
	var res = '';
	if(ua.indexOf("iphone")>-1){res = 'iphone';}else if(ua.indexOf("ipad")>-1){res = 'ipad';}else if(ua.indexOf("android")>-1){res = 'android';}else if(ua.indexOf("ucweb")>-1){res = 'ucweb';}else if(ua.indexOf("windows phone")>-1){res = 'Windows Phone';}else if(ua.indexOf("symbian")>-1){res = 'Symbian';}
	//alert(ua);
	return res;
}
