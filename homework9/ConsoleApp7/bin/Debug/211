 function czodYsSearch(type,objectID)
 {
   if (objectID == "" || objectID == null)
   {
     return;
   }
   var czodID = document.getElementById(objectID).value;

   if (czodID == "" || czodID == "0")
   {
     alert("��ѡ����Ф");
           return;
   }

   var now = new Date();
   var year = now.getFullYear();
   var month = now.getMonth() + 1;
   var day = now.getDate();
   
   //�洢������ص���Ϣ
   var czodInfo = {
     "Week" : "weekczod",
     "Nweek" : "weekczod",
     "Month" : "monthczod",
     "Year" : "yearczod"
   };

   //������£��Ҳ�����js����̫��
   var randStr = "" + now.getFullYear() + "" + (now.getMonth() + 1) + "" + now.getDate();

   var urljs = "//data.astro.qq.com/czodinfo/" + czodID + "/todayCzod.js?" + randStr;

   if (typeof todayCzod == "undefined")
   {
     JsLoader.load(urljs, function()
     {
       if (typeof todayCzod != "undefined")
       {
         //��ö�Ӧ��FID
         var FID = todayCzod[type + "FID"];
         if (FID == null || FID == "")
         {
           alert("���޸����ݣ��Ժ���䣬����ʱ��ע��������");
         }else
         {
           var urlPage = "//data.astro.qq.com/" + czodInfo[type] + "/" + Math.floor(FID / 1000);
           urlPage += "/" + FID + "/index.shtml";
           //location.href = urlPage;
           window.open(urlPage);
         }
       }
     });
   }else
   {
     //�����Ѿ���ȡ�ɹ�
     var FID = todayCzod[type + "FID"];
     if (FID == null || FID == "")
     {
       alert("���޸����ݣ��Ժ���䣬����ʱ��ע��������");
     }else
     {
       var urlPage = "//data.astro.qq.com/" + czodInfo[type] + "/" + Math.floor(FID / 1000);
       urlPage += "/" + FID + "/index.shtml";
       window.open(urlPage);
     }
   }
 }
 
