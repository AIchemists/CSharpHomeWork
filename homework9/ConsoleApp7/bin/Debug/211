<script type="text/javascript">
	function dotScroll(cContainer,cNum,sNum,cDot,cCurLeft,cCurRight,autoScroll){
		this.container=cContainer;
		this.dot=cDot;
		this.curLeft=cCurLeft;
		this.curRight=cCurRight;
		this.sNum=cNum;
        this.auto=autoScroll;
		this.tabNum=sNum;

        this.currentNode=0;
		this.oldNode=0;
        this.dur=0;
		this.ready=null;
		
		
	}
	dotScroll.prototype = {
	    init: function () {
	        var sItem = document.getElementById(this.container).getElementsByTagName('li')[0].offsetWidth;
	        this.dur = sItem * this.sNum;
			
	        var allNum = document.getElementById(this.container).getElementsByTagName('li').length;

	        var allLength = sItem * allNum;
			
			var temp=document.createElement('div');
			temp.innerHTML=document.getElementById(this.container).getElementsByTagName('div')[0].innerHTML;
			document.getElementById(this.container).getElementsByTagName('div')[0].appendChild(temp);
			
	        document.getElementById(this.container).getElementsByTagName('div')[0].style.width = allLength*2+1000 + 'px';
			
			var flag0=flag1=flag2=flag3=flag4=flag5=flag6=null;
			
			var _this=this;
			if(this.auto=='true'){
				if(flag0||flag1||flag2||flag3||flag4||flag5||flag6){
					clearInterval(flag0);
					clearInterval(flag1);
					clearInterval(flag2);
					clearInterval(flag3);
					clearTimeout(flag4);
					clearTimeout(flag5);
					clearTimeout(flag6);
				}
				flag0=setInterval(function(){
					_this.act('next');
				},5000)
			}
			
			document.getElementById(this.curLeft).onclick=function(){
				_this.act('prev');
				if(_this.auto=='true'){
					if(flag0||flag1||flag2||flag3||flag4||flag5||flag6){
					clearInterval(flag0);
					clearInterval(flag1);
					clearInterval(flag2);
					clearInterval(flag3);
					clearTimeout(flag4);
					clearTimeout(flag5);
					clearTimeout(flag6);
				}
				flag4=setTimeout(function(){
				flag1=setInterval(function(){
					_this.act('next');
				},5000)
			},10000)
			}
				return false;
				
			}
			
			document.getElementById(this.curRight).onclick=function(){
				_this.act('next');
				if (_this.auto == 'true') {
					if (flag0 || flag1 || flag2 || flag3 || flag4 || flag5 || flag6) {
						clearInterval(flag0);
						clearInterval(flag1);
						clearInterval(flag2);
						clearInterval(flag3);
						clearTimeout(flag4);
						clearTimeout(flag5);
						clearTimeout(flag6);
					}
					flag5 = setTimeout(function(){
						flag2 = setInterval(function(){
							_this.act('next');
						}, 5000)
					}, 10000)
				}
				return false;
				
			}
			
			var dotNode=document.getElementById(this.dot).getElementsByTagName('a');
			for(var i=0;i<this.tabNum;i++){
				dotNode[i].onclick=(function(a){
					return function(){

						_this.currentNode=a;
						_this.scroll('noDir');
						_this.dotCurrent();
						if (_this.auto == 'true') {
							if (flag0 || flag1 || flag2 || flag3 || flag4 || flag5 || flag6) {
										clearInterval(flag0);
										clearInterval(flag1);
										clearInterval(flag2);
										clearInterval(flag3);
										clearTimeout(flag4);
										clearTimeout(flag5);
										clearTimeout(flag6);
									}
							flag6 = setTimeout(function(){
								
								flag3 = setInterval(function(){
									
									_this.act('next');
								}, 5000)
							}, 10000)
						}
						return false;
						}
					})(i);
			}
	    },
		tween:function(a,b,l,t){
			return a+b*t/l;
		},
		opa:function(){
			var m=0
			
			var sflag2=0;
			var _this=this;
			
			(function(){
				m++;
				document.getElementById(_this.container).getElementsByTagName('div')[0].style.opacity=m/10;
				document.getElementById(_this.container).getElementsByTagName('div')[0].style.filter='alpha(opacity='+m*10+')';
				
				sflag2=setTimeout(arguments.callee,30);
	
				if(m>10){
					clearTimeout(sflag2);
					document.getElementById(_this.container).getElementsByTagName('div')[0].style.opacity=1;
					document.getElementById(_this.container).getElementsByTagName('div')[0].style.filter=(opacity=100);
					_this.ready=null;
				}
				
			})()
			
			
		},
	    scroll: function (dir) { 
			var s=this.dur*this.currentNode;
			
			if(dir=='noDir'){
				
				document.getElementById(this.container).getElementsByTagName('div')[0].style.left=-s+'px';
				document.getElementById(this.container).getElementsByTagName('div')[0].opacity=0;
				this.opa();
				return;
			}
			
			var t=0;
			var durTime=30;
			var _this=this;
			var startPos=this.oldNode*this.dur;
			var change=(this.currentNode-this.oldNode)*this.dur;
			var sflag=null;
			
			if(dir=='next'&&this.oldNode==(this.tabNum-1)){
				startPos=(this.tabNum-1)*this.dur;
				change=this.dur;
			}else if(dir=='prev'&&this.oldNode==0){
				startPos=this.tabNum*this.dur;
				change=-this.dur;
			}

			
			(function(){
				t++
				
				var innerChange=_this.tween(startPos,change,durTime,t);

				document.getElementById(_this.container).getElementsByTagName('div')[0].style.left=-innerChange+'px';
				
				sflag=setTimeout(arguments.callee,30);
				
				if(t>durTime){
					clearTimeout(sflag);
					sflag=null;
					document.getElementById(_this.container).getElementsByTagName('div')[0].style.left=-s+'px';
					_this.ready=null;
				}
			})()
			
			
        },
		act:function(dir){
			if(this.ready) return;
			
			this.ready=true;
			var max=this.tabNum-1;
			var current=this.currentNode;
			if(dir=='prev'){
				current=current-1;
				if(current<0){
					current=max;
				}
			}else if(dir=='next'){
				current=current+1;
				if(current>max){
					current=0;
				}
			}
			this.oldNode=this.currentNode;
			this.currentNode=current;
			
			this.scroll(dir);
			this.dotCurrent();
		},
		dotCurrent:function(){
			var dotNode=document.getElementById(this.dot).getElementsByTagName('a');
			for(var i=0;i<this.tabNum;i++){
				dotNode[i].className='';
				if(i==this.currentNode){
					dotNode[i].className='on';
				}
			}
		}
	}
	
	var dotScroll=new dotScroll('scrollBox',5,6,'dot','leftCur','rgtCur','false');
	dotScroll.init();
</script>
