<script type="text/javascript">
	function hideShowCard(oId){
		this.container=document.getElementById(oId);
	}
	hideShowCard.prototype={
		init:function(){
			var dd=this.container.getElementsByTagName('dd');
			var dt=this.container.getElementsByTagName('dt');
			
			var cNum=dt.length;
			
			var current=this.currentNode;
			var _this=this;
	
			for(var i=0;i<cNum;i++){
				dd[i].onmouseover=(function(a){
					return function(){
						for (var j = 0; j < cNum; j++) {
							dd[j].style.display = 'block';
							dt[j].style.display = 'none';



						}
						dt[a].style.display='block';
						dd[a].style.display='none';
					
					}
				})(i)
			}
		}
	}
	var card0=new hideShowCard('tabCard1');
	card0.init();
	
	var card1=new hideShowCard('tabCard2');
	card1.init();

</script>


