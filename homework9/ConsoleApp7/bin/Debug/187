var cache = {};
var arrTree = [];
var Cn2PinYin;
function _set_bin_tree(a,b)
{
	var c = Math.floor((a+b) / 2);
	if(c==b && b<a)			//�ڵ�
	{
		arrTree.push("r='", auto_py_pinyin[c], "';");
		return;
	}
	arrTree.push(			//���֧
	"if(w.localeCompare('", auto_py_key[c], "')<0)");
	_set_bin_tree(a, c-1, 1);
	arrTree.push("else ");	//�ҷ�֧
	_set_bin_tree(c+1, b, 2);
}
function set_pinyin_init()
{
	arrTree.push("var r=cache[w];if(!r)");				//��黺��
	_set_bin_tree(0, auto_py_key.length-1);								//-�ݹ�����Դ��
	arrTree.push("return cache[w]=r;");					//-д�뻺��
	Cn2PinYin = new Function("w", arrTree.join(""));	//����
}
function get_cur_pinyin(content)
{
	var fn = Cn2PinYin;
	var arr = [];
	var ch;
	for(var i=0,n=content.length; i<n; i++)
	{
		ch = content.charAt(i);
		arr[i] = (ch<"һ" || ch>"��")?ch:fn(ch);
	}
	return arr.join("");
}
