function autoBussBoss(ev,clicktype)
{
	
    var loopTryNum = 10;
    var autoBossID= 2586; 
    try
	{		
    	
		var a=document.cookie.match(new RegExp('(^|)o_cookie=([^;]*)(;|$)'));
		var iQQ=(a==null?"":unescape(a[2]));
		var purl='';
		var autoBussBoss = '';
		var ev = window.event || ev;
		var et = ev.srcElement || ev.target;
		if(typeof clicktype == 'undefined'){
	       
	       
		    var type=et.tagName;
	        if (type != "A" && type != "IMG" ) 
		    {
		        return true;
		    } 
		
		    if (type == "A")
		    {
		        purl = et.href;
		    }
		    else if (type == "IMG")
		    {
		        purl = et.parentNode.href;
		    }
		    
			for (var i=loopTryNum-1,tagNode=et;i>=0;i--,tagNode=tagNode.parentNode)
		    {
				autoBussBoss = tagNode.getAttribute('autobussboss');
		        if(autoBussBoss) break; 
		    }	
			
		    if(!autoBussBoss) return;
		}else{
			for (var i=loopTryNum-1,tagNode=et;i>=0;i--,tagNode=tagNode.parentNode)
		    {
				autoBussBoss = tagNode.getAttribute('autobussboss');
		        if(autoBussBoss) break; 
		    }
			purl = '';
			sFlag = true;
		}
		var localUrl = location.href;
		var site = localUrl.substring(7,localUrl.indexOf('.qq.com'));
		site = site.substr(site.lastIndexOf('.')+1);
		if(autoBussBoss){

			autoBossImg = new Image();
			var autoBossData = autoBussBoss.split('|');

			var timestamp = Date.parse(new Date());

			var autourl = '//btrace.qq.com/collect?ftime=&sIp=&iQQ='+iQQ+'&sBiz=dealerExposure&sOp='+autoBossData[0]+'&iSta=&iTy='+autoBossID+'&iFlow='+timestamp+'&sUrl='+escape(localUrl)+'&sRef='+escape(purl)+'&sPageId='+autoBossData[2]+'&sPos='+autoBossData[1];
			autoBossImg.src = autourl;
		}


	} catch (e) {}
}

if(document.addEventListener){
	document.addEventListener("click", autoBussBoss, false);}
else if(document.attachEvent){
	document.attachEvent("onclick", autoBussBoss);}
    
</script>
